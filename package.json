    "babel-loader": "^9.1.3",
    "concurrently": "^5.2.0",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^2.0.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-cli": "^7.0.0",
    "workbox-webpack-plugin": "^7.0.0"
